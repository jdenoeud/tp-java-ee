<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>tpFilRouge</display-name>
  <servlet>
    <servlet-name>Client</servlet-name>
    <servlet-class>com.sdz.servlets.CreationClient</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Client</servlet-name>
    <url-pattern>/creationClient</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>Commande</servlet-name>
    <servlet-class>com.sdz.servlets.CreationCommande</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Commande</servlet-name>
    <url-pattern>/creationCommande</url-pattern>
  </servlet-mapping>
  
  <jsp-config>
		<jsp-property-group>
			<url-pattern>*.jsp</url-pattern>
			<include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
		</jsp-property-group>
	</jsp-config>
</web-app>